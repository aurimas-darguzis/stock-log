{
  "name": "stock-log",
  "version": "0.1.0",
  "author": {
    "name": "AD Programming",
    "email": "aurimas@adprogramming.com",
    "url": "https://reacting.io"
  },
  "build": {
    "appId": "com.reacting.stock-log",
    "productName": "StockLog",
    "copyright": "Copyright Â© 2019 ${author}",
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.2",
    "apollo-boost": "^0.4.4",
    "clsx": "^1.0.4",
    "electron-is-dev": "^1.1.0",
    "graphql": "^14.5.8",
    "history": "^4.10.0",
    "react": "^16.9.0",
    "react-apollo": "^3.1.2",
    "react-dom": "^16.9.0",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.6.1"
  },
  "devDependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@rescripts/cli": "^0.0.12",
    "@rescripts/rescript-env": "^0.0.10",
    "concurrently": "^4.1.2",
    "cross-env": "^6.0.0",
    "electron": "^6.0.10",
    "electron-builder": "^21.2.0",
    "material-table": "^1.50.0",
    "react-scripts": "3.1.1",
    "typescript": "^3.6.3",
    "wait-on": "^3.3.0"
  },
  "scripts": {
    "start": "rescripts start",
    "build": "rescripts build",
    "test": "rescripts test",
    "eject": "react-scripts eject",
    "electron-dev": "concurrently \"BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build",
    "electron-pack": "electron-builder -mw"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "rescripts": [
    "env"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
